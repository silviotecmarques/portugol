algoritmo "Dobro e Triplo de um numero"
var
      numero, valor1, valor2: real
inicio
      escreval("Digite um número:")
      leia(numero)
      valor1<- numero*2
      valor2<- numero*3
      escreval("O dobro de ",numero," é ",valor1)
      escreval("O triplo de ",numero," é ",valor2)

fimalgoritmo